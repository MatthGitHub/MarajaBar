<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scenes.proveedores.ProveedoresController">
    <stylesheets>
        <URL value="@proveedores.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <Pane prefHeight="96.0" prefWidth="600.0">
               <children>
                  <Label layoutX="213.0" layoutY="7.0" prefHeight="34.0" prefWidth="200.0" text="Proveedores">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <ImageView fitHeight="50.0" fitWidth="83.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../resources/images/proveedores.jpeg" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
            <TabPane prefHeight="335.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Proveedores">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TableView fx:id="tblProveedores" prefHeight="205.0" prefWidth="600.0">
                                <columns>
                                  <TableColumn fx:id="colNombre" prefWidth="177.0" text="Nombre" />
                                  <TableColumn fx:id="colCuit" prefWidth="105.0" text="Cuit" />
                                    <TableColumn fx:id="colTelefono" prefWidth="148.0" text="Telefono" />
                                    <TableColumn fx:id="colMail" prefWidth="169.0" text="Mail" />
                                </columns>
                              </TableView>
                              <Button fx:id="btnEliminar" layoutX="271.0" layoutY="215.0" mnemonicParsing="false" onAction="#eliminarProveedor" text="Eliminar" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab onClosed="#limpiarModificar" onSelectionChanged="#modificarProveedor" text="Modificar">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <Label fx:id="lblProveedor" layoutX="120.0" layoutY="24.0" prefHeight="29.0" prefWidth="376.0">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="txtNombreMod" layoutX="37.0" layoutY="76.0" prefHeight="27.0" prefWidth="232.0" promptText="Nombre proveedor" />
                              <TextField fx:id="txtCuitMod" layoutX="37.0" layoutY="115.0" prefHeight="27.0" prefWidth="232.0" promptText="C.U.I.T. proveedor" />
                              <TextField fx:id="txtCorreoMod" layoutX="300.0" layoutY="76.0" prefHeight="27.0" prefWidth="229.0" promptText="Correo proveedor" />
                              <TextField fx:id="txtTelefonoMod" layoutX="300.0" layoutY="115.0" prefHeight="27.0" prefWidth="229.0" promptText="Telefono proveedor" />
                              <Button fx:id="btnGuardarMod" layoutX="249.0" layoutY="178.0" mnemonicParsing="false" onAction="#guardarProveedorMod" text="Guardar" />
                           </children></AnchorPane>
                  </content>
                </Tab>
                  <Tab text="Nuevo">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TextField fx:id="txtNombre" layoutX="39.0" layoutY="44.0" prefHeight="27.0" prefWidth="216.0" promptText="Nombre proveedor" />
                              <TextField fx:id="txtCuit" layoutX="39.0" layoutY="101.0" prefHeight="27.0" prefWidth="214.0" promptText="C.U.I.T. proveedor" />
                              <TextField fx:id="txtCorreo" layoutX="336.0" layoutY="44.0" prefHeight="27.0" prefWidth="214.0" promptText="Correo proveedor" />
                              <TextField fx:id="txtProveedor" layoutX="335.0" layoutY="101.0" prefHeight="27.0" prefWidth="216.0" promptText="Telefono proveedor" />
                              <Button fx:id="btnGuardar" layoutX="271.0" layoutY="182.0" mnemonicParsing="false" onAction="#guardarProveedor" text="Guardar" />
                           </children></AnchorPane>
                    </content>
                  </Tab>
              </tabs>
            </TabPane>
            <Pane prefHeight="106.0" prefWidth="600.0">
               <children>
                  <Button fx:id="btnVolver" layoutX="14.0" layoutY="17.0" mnemonicParsing="false" onAction="#volver" text="Volver" />
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
