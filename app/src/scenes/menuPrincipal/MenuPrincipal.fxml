<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" scaleShape="false" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scenes.menuPrincipal.MenuPrincipalController">
    <stylesheets>
        <URL value="@menuprincipal.css" />
    </stylesheets>
   <children>
      <VBox fx:id="vbox" prefHeight="400.0" prefWidth="600.0">
         <children>
            <MenuBar fx:id="menuVentana" onDragDetected="#moverVentana" prefHeight="29.0" prefWidth="600.0">
              <menus>
                <Menu mnemonicParsing="false">
                     <graphic>
                        <Button fx:id="btnMaximizar" mnemonicParsing="false" onAction="#maximizarVentana" text="[]" />
                     </graphic>
                </Menu>
                  <Menu mnemonicParsing="false">
                     <graphic>
                        <Button fx:id="btnIconizar" mnemonicParsing="false" onAction="#iconizarVentana" text="-" />
                     </graphic>
                  </Menu>
              </menus>
            </MenuBar>
            <Pane prefHeight="28.0" prefWidth="600.0">
               <children>
                  <MenuBar layoutY="2.0" prefHeight="24.0" prefWidth="600.0">
                    <menus>
                      <Menu mnemonicParsing="false" text="Archivo">
                        <items>
                          <MenuItem fx:id="btnSalir" mnemonicParsing="false" onAction="#salir" text="Salir" />
                        </items>
                      </Menu>
                        <Menu mnemonicParsing="false" text="Ver">
                          <items>
                            <MenuItem mnemonicParsing="false" text="Ventas" />
                          </items>
                        </Menu>
                      <Menu mnemonicParsing="false" text="Opciones">
                        <items>
                          <MenuItem mnemonicParsing="false" text="Proveedores" />
                              <MenuItem mnemonicParsing="false" text="Mesas" />
                              <MenuItem mnemonicParsing="false" text="Productos" />
                              <MenuItem mnemonicParsing="false" text="Usuarios" />
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Ayuda">
                        <items>
                          <MenuItem mnemonicParsing="false" text="Acerca" />
                        </items>
                      </Menu>
                    </menus>
                  </MenuBar>
               </children>
            </Pane>
            <Pane prefHeight="73.0" prefWidth="600.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" layoutX="128.0" layoutY="6.0" prefHeight="14.0" prefWidth="287.0" text="Mesas">
                     <font>
                        <Font name="Arial" size="48.0" />
                     </font>
                  </Label>
               </children></Pane>
            <FlowPane fx:id="flwPane" prefHeight="272.0" prefWidth="600.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
